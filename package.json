{
  "name": "qa-dashboard",
  "version": "0.0.1",
  "engines": {
    "node": ">=0.8.0"
  },
  "dependencies":{
    "express": "3.2.0",
    "async": "0.2.7",
    "underscore": "1.4.4",
    "mongodb": "1.2.14",
    "coffee-script": "1.6.2",
    "less": "1.3.3",
    "connect-mongo": "0.3.2",
    "connect-coffee-script": "0.1.3",
    "connect-less": "0.3.1",
    "passport": "0.1.16",
    "passport-local": "0.1.6",
    "passport-ldapauth": "0.1.0",
    "bcrypt": "0.7.5",
    "commander": "1.1.1"
  },

  "devDependencies": {
    "vows": "0.7.0",
    "nodeunit": "0.8.0",
    "chai": "1.5.0",
    "mocha": "1.9.0",
    "supertest": "0.6.0",
    "supervisor": "0.5.2"
  },

  "scripts": {
    "start":   "NODE_PATH=server NODE_ENV=production coffee qadash.coffee",
    "test":    "NODE_PATH=server NODE_ENV=test nodeunit test/test-*; NODE_PATH=server NODE_ENV=test mocha --compilers coffee:coffee-script test/*-test.coffee",

    "createuser": "NODE_PATH=server coffee server/user_admin.coffee",

    "dev-start": "NODE_PATH=server NODE_ENV=development supervisor -w qadash.coffee,server,plugins/query,plugins/import,settings.json -n -e coffee,json -x coffee qadash.coffee"
  },

  "private": "true"
}
