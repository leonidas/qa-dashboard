    "node": ">=0.8.0"
  },
  "dependencies":{
    "express": "3.1.1",
    "async": "0.2.6",
    "underscore": "1.4.4",
    "mongodb": "1.2.14",
    "coffee-script": "1.6.2",
    "less": "1.3.3",
    "connect-mongo": "0.3.2",
    "mysql": "0.9.1",
    "connect-coffee-script": "0.1.3",
    "connect-less": "0.3.1",
    "passport": "0.1.16",
    "passport-local": "0.1.6"
  },

  "devDependencies": {
    "vows": "0.7.0",
    "nodeunit": "0.8.0",
    "should": "1.2.2",
    "supervisor": "0.5.2"
